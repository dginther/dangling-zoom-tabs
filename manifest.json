{
  "manifest_version": 2,
  "name": "Dang(ling) Zoom Tab",
  "description": "Closes out that dang(ling) Zoom tab.",
  "version": "0.1.0",
  "icons": {
    "128": "logo.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://zoom.us/j/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs", "https://zoom.us/j/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
